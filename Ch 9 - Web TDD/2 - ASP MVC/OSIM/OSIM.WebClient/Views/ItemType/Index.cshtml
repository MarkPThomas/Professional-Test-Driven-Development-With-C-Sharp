@model IEnumerable<OSIM.Core.Entities.ItemType>

@{
    ViewBag.Title = "ItemType";
}

<h2>ItemType</h2>

<p>@Html.ActionLink("Create New", "Create")</p>
<table>
    <tr>
        <th></th>
        <th>Name</th>
    </tr>
    @foreach (var item in Model) {
        <tr>
            <td>@Html.ActionLink("Edit", "Edit", new { id=item.Id }) |</td>
            <td>@item.Name</td>
        </tr>
    }
</table>

